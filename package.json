{
    "name": "tweeter-backend",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Wesley Westelley <wesleywestelley@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
        "start": "node dist/src/server.js",
        "build": "tsc",
        "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
        "migration:run:production": "node ./node_modules/typeorm/cli.js migration:run -d dist/src/api/database/index.js",
        "test": "jest --runInBand",
        "test:watch": "jest --no-cache --watchAll",
        "test:coverage": "jest --no-cache --coverage"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/multer": "^1.4.7",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "eslint": "^8.12.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-import-helpers": "^1.2.1",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.6.1",
        "ts-jest": "^27.1.4",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "celebrate": "^15.0.1",
        "cloudinary": "^1.30.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.4",
        "multer-storage-cloudinary": "^4.0.0",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.5.0",
        "typeorm": "^0.3.4"
    }
}
